{
  "parser": "babel-eslint",
  "extends": ["airbnb-base", "plugin:node/recommended"],
  "plugins": [
    "babel"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true,
    "jest": true,
    "jasmine": true
  }, 
  "globals": {
    "res": true,
    "redirect": true,
    "app": true,
    "view": true
  },
  "rules": {
    "no-restricted-syntax": [0]
  }
}